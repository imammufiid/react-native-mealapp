{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@data/*": [
        "src/data/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
